    "version": "0.2.0",
    "configurations": [
        {
            "name": "[GDB] app_MAIN < debug.in",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/app_MAIN",
            "args": [ "<", "${workspaceFolder}/instances/grafos/grafo_17.txt", ">", "${workspaceFolder}/workspace/debug.out", "-r", "4", "-c", "10", "-t", "16", "--debug", "--file" ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
