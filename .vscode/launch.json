    "version": "0.2.0",
    "configurations": [
        {
            "name": "[GDB] app_SEQ-OLD",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/app_SEQ-OLD",
            "args": [ ">", "${workspaceFolder}/workspace/debug.out" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "[GDB] app_PAR-OLD",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/app_PAR-OLD",
            "args": [ ">", "${workspaceFolder}/workspace/debug.out" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "[GDB] app_BF-SEQ < debug.in",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/app_BF-SEQ",
            "args": [ "<", "${workspaceFolder}/workspace/debug.in", ">", "${workspaceFolder}/workspace/debug.out" ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}